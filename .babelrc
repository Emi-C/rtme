{
    "presets": [
      "env","react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-runtime",
      ["module-resolver", {
        "root": ["./src"],
        "alias":{
          "Auth":"./src/Auth",
          "App":"./src/App",
          "Home":"./src/Home",
          "Login":"./src/Login",
          "Profile":"./src/Profile",
          "reduxUtils":"./src/reduxUtils",
          "router":"./src/router",
          "theme":"./src/theme"
        }
      }]
    ]
  }